  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only --exit-child -r tsconfig-paths/register src/server.ts",
    "build": "tsc && tsc-alias",
    "lint": "eslint . --ext .ts",
    "migrate": "node-pg-migrate up --database-url-var DB_URL --config-file ./src/db/pg-migrate.config.json",
    "lint-fix": "eslint . --ext .ts --fix",
    "migrate_local": "node-pg-migrate up --database-url-var DB_LOCAL_URL --config-file ./src/db/pg-migrate.config.json",
    "down_local_migrate": "node-pg-migrate down --database-url-var DB_LOCAL_URL --config-file ./src/db/pg-migrate.config.json"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.11.19",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "node-pg-migrate": "^8.0.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.2.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.3",
    "pg": "^8.16.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "uuid": "^11.1.0",
    "zod": "^3.24.3"
  }
}
